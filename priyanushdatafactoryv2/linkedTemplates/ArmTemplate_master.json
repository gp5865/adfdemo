{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "priyanushdatafactoryv2"
		},
		"Blob_linkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blob_linkedservice'"
		},
		"linkedservice_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-PRIYANUSH.vault.azure.net/"
		},
		"linkedservice_BlobStorage_KV_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-priyanushstorageaccount"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "priyanushdatafactoryv2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Blob_linkedservice_connectionString": {
						"value": "[parameters('Blob_linkedservice_connectionString')]"
					},
					"linkedservice_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('linkedservice_kv_properties_typeProperties_baseUrl')]"
					},
					"linkedservice_BlobStorage_KV_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('linkedservice_BlobStorage_KV_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}